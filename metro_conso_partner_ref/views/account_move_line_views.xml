<odoo>
	<!-- TREE VIEW -->
	<record model="ir.ui.view" id="metro_view_partner_form_conso_partner_move_line_tree">
		<field name="name">metro_view_partner_form_conso_partner_move_line_tree</field>
		<field name="model">account.move.line</field>
		<field name="inherit_id" ref="account.view_move_line_tree" />
		<field name="priority" eval="22" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='partner_id']" position="after">
                    <field name="x_conso_partner_ref"/>
            </xpath>
		</field>
	</record>
    <!-- FORM VIEW -->
	<record model="ir.ui.view" id="metro_view_partner_form_conso_partner_move_line_form">
		<field name="name">metro_view_partner_form_conso_partner_move_line_form</field>
		<field name="model">account.move.line</field>
		<field name="inherit_id" ref="account.view_move_line_form" />
		<field name="priority" eval="22" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='partner_id']" position="after">
                    <field name="x_conso_partner_ref"/>
            </xpath>
		</field>
	</record>
</odoo>
