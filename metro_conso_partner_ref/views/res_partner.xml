<odoo>
	<!-- FORM VIEW -->
	<record model="ir.ui.view" id="metro_view_partner_form_conso_partner">
		<field name="name">metro_view_partner_form_conso_partner</field>
		<field name="model">res.partner</field>
		<field name="inherit_id" ref="account.view_partner_property_form" />
		<field name="priority" eval="22" />
		<field name="arch" type="xml">
			<xpath expr="//group[@name='fiscal_information']" position="after">
                <group string="Consolidation">
                    <field name="x_conso_partner_ref"/>
                </group>
            </xpath>
		</field>
	</record>
</odoo>
